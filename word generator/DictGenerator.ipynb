   "source": [
    "with open('ne.txt', 'r') as f:\n",
    "    lines = f.readlines()\n",
    "\n",
    "# remove spaces\n",
    "lines = [line.replace(' ', '\\n') for line in lines]\n",
    "\n",
    "#delete empty line\n",
    "lines = [line for line in lines if line.strip()]\n",
    "\n",
    "with open('ne.txt', 'w') as f:\n",
    "    f.writelines(lines)"
   ]
  }
